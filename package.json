{
  "name": "voynexus",
  "version": "0.1.0",
  "private": true,
  "description": "Voynex travel OS for Tokushima with microCMS integration, AI itinerary planner, and facility widget.",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.11.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx,.mts,.cts --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "format": "prettier --write .",
    "prepare": "husky install",
    "analyze": "ANALYZE=true next build",
    "sitemap": "tsx scripts/build-sitemap.ts",
    "embeddings": "tsx scripts/build-embeddings.ts",
    "og": "tsx scripts/build-og.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.10",
    "@supabase/supabase-js": "^2.45.0",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "lucide-react": "^0.462.0",
    "microcms-js-sdk": "^3.2.0",
    "next": "14.2.5",
    "next-intl": "^3.18.0",
  "next-auth": "^4.24.13",
    "openai": "^4.56.3",
    "pdf-lib": "^1.17.1",
    "postcss": "^8.4.47",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "stripe": "^19.3.0",
    "swr": "^2.2.5",
    "tailwindcss": "^3.4.13",
    "xss": "^1.0.15",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.16.7",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.6",
    "jsdom": "^27.1.0",
    "lint-staged": "^15.2.10",
    "msw": "^2.5.2",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^2.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cts,mts}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,js,jsx,json,md,css}": [
      "prettier --write"
    ]
  }
}
